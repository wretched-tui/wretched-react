{
  "name": "wretched-react",
  "description": "React Reconciler and renderer for Wretched",
  "author": "Colin T.A. Gray <colinta@colinta.com>",
  "contributors": [
    "Shawn Chen <meishangchen@gmail.com>"
  ],
  "version": "1.0.5",
  "license": "MIT",
  "preferGlobal": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/wretched-tui/wretched-react.git"
  },
  "homepage": "https://github.com/wretched-tui/wretched-react",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "bugs": {
    "url": "http://github.com/wretched-tui/wretched-react/issues"
  },
  "scripts": {
    "prepublishOnly": "yarn clean && yarn tsc && rm -rf dist/demos/",
    "postpublish": "yarn run gittag",
    "gittag": "git tag $(node -e 'process.stdout.write(require(\"./package\").version)')",
    "clean": "rm -rf dist/",
    "build": "yarn clean && yarn tsc",
    "checksum": ". scripts/check.sh lib",
    "demo": "yarn checksum && cd demos/ && yarn demo"
  },
  "keywords": [
    "react",
    "curses",
    "tui",
    "terminal",
    "terminal-ui"
  ],
  "tags": [
    "react",
    "curses",
    "tui",
    "terminal"
  ],
  "engines": {
    "node": ">= 18.12.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-reconciler": "^0.29.0"
  },
  "dependencies": {
    "preact": "^10.24.3",
    "wretched": "^1.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.39",
    "@types/react-reconciler": "^0.28.8",
    "ssed": "^1.5.0",
    "typescript": "^5.6.3"
  }
}
